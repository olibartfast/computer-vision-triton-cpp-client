    "version": "0.2.0",
    "configurations": [
        {
        "name": "C++ Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/computer-vision-triton-cpp-client",
        "args": ["--source=data/mug.jpg", 
        "--serverAddress=192.168.1.212",
        "--labelsFile=labels/imagenet.txt",
        "--model=resnet50_trt",
        "--model_type=torchvision-classifier",
        "--task_type=classification",
        "--input_sizes=3 224 224"],
        "environment": [{ "name": "config", "value": "Debug" }],
        "cwd": "${workspaceFolder}" 
        }

    ]
}